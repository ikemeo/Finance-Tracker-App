# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/portfolio_dashboard

# Plaid Integration (Optional)
PLAID_CLIENT_ID=your_plaid_client_id_here
PLAID_SECRET=your_plaid_secret_key_here
PLAID_ENV=sandbox

# E*TRADE Integration (Optional)
ETRADE_CONSUMER_KEY=your_etrade_consumer_key_here
ETRADE_CONSUMER_SECRET=your_etrade_consumer_secret_here
ETRADE_PRODUCTION_CONSUMER_KEY=your_production_key_here
ETRADE_PRODUCTION_CONSUMER_SECRET=your_production_secret_here

# Alternative Brokerage APIs (Optional)
ROBINHOOD_USERNAME=your_robinhood_username
ROBINHOOD_PASSWORD=your_robinhood_password
SCHWAB_CLIENT_ID=your_schwab_client_id
SCHWAB_CLIENT_SECRET=your_schwab_client_secret

# Application Configuration
NODE_ENV=development
PORT=5000

# Session Configuration (for production)
SESSION_SECRET=your_secure_session_secret_here

# PostgreSQL Direct Connection (Alternative to DATABASE_URL)
PGHOST=localhost
PGPORT=5432
PGUSER=username
PGPASSWORD=password
PGDATABASE=portfolio_dashboard